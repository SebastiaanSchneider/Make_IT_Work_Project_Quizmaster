<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0"
            xmlns="http://javafx.com/javafx/17.0.2"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.WelcomeController">
    <Label fx:id="welcomeLabel" layoutX="14.0" layoutY="14.0"
           text="Welkom gebruiker">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Label layoutX="14.0" layoutY="72.0" text="Wat wilt u doen">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Label fx:id="nameRoleLabel" layoutX="701.0" layoutY="14.0"
           prefHeight="66.0" prefWidth="185.0" text="Naam">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>

    <MenuButton fx:id="taskMenuButton" layoutX="191.0" layoutY="70.0"
                mnemonicParsing="false" text="Kies taak">
        <font>
            <Font size="14.0"/>
        </font>
    </MenuButton>
    <Button layoutX="14.0" layoutY="121.0" mnemonicParsing="false"
            onAction="#doLogout" text="Log uit">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>

</AnchorPane>
